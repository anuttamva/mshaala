<body class="dd"  style="background-color:#F5F5F5;padding-top:1px;" style="background-image:url("1.jpg")">
  	<!-- Display flash messages  -->
	<% for(var i in messages) { %>
	   <div class="flash"><%= messages[i] %></div>
	<% } %>

	<!-- Render the signin form  -->
    
    
    
    
    
    <md-card  style="background-color;border:50px;margin-left:24%;margin-right:24%;margin-top:15%;">
        <md-card-content  style="background-color:white;" align="center">
            <form action="/signin" method="post">
                <label style="color:red">Username : </label>
                <md-input-container>
                    <div>
                        <input type="text" name="username" placeholder="Username"/>
                    </div>
                </md-input-container><br>
                <label style="color:red">Password : </label>
                <md-input-container>
                    <div>
                        <input type="password" name="password" placeholder="Password"/>
                    </div>
                </md-input-container>

                <div align="center">
                    <md-button>
                        <input type="submit" style="background-color: lightgrey;border: none;color: red;padding: 15px 32px;text-align: center;text-decoration: none;display: inline-block;font-size: 16px;margin: 4px 2px;cursor:pointer;border-radius: 6px;" value="Sign In" />
                    </md-button>

                </div>
            </form>
        </md-card-content>
    </md-card>

	<!-- Render the OAuth authentication links  -->
<!--
	<a href="/oauth/facebook">Sign in with Facebook</a>
	<a href="/oauth/twitter">Sign in with Twitter</a>
	<a href="/oauth/google">Sign in with Google</a>
    
-->
    
